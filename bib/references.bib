  author = {Davis, K. B. and Mewes, M. O. and Andrews, M. R. and Van Druten, N. J. and Durfee, D. S. and Kurn, D. M. and Ketterle, W.},
  year = {1995},
  month = nov,
  journal = {Physical Review Letters},
  volume = {75},
  number = {22},
  pages = {3969--3969},
  publisher = {{American Physical Society}},
  doi = {10.1103/PhysRevLett.75.3969},
  abstract = {We have observed Bose-Einstein condensation of sodium atoms. The atoms were trapped in a novel trap that employed both magnetic and optical forces. Evaporative cooling increased the phase-space density by 6 orders of magnitude within seven seconds. Condensates contained up to 5\texttimes 105 atoms at densities exceeding 1014 cm-3. The striking signature of Bose condensation was the sudden appearance of a bimodal velocity distribution below the critical temperature of 2K. The distribution consisted of an isotropic thermal distribution and an elliptical core attributed to the expansion of a dense condensate. \textcopyright{} 1995 The American Physical Society.}
}

