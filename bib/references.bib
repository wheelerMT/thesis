@article{Hendry1994,
  title = {Generation of Defects in Superfluid {{4He}} as an Analogue of the Formation of Cosmic Strings},
  author = {Hendry, P. C. and Lawson, N. S. and Lee, R. A.M. and McClintock, P. V.E. and Williams, C. D.H.},
