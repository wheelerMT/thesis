@article{Villois2016,
  title = {A Vortex Filament Tracking Method for the {{Gross}}\textendash{{Pitaevskii}} Model of a Superfluid},
  author = {Villois, Alberto and Krstulovic, Giorgio and Proment, Davide and Salman, Hayder},
  year = {2016},
