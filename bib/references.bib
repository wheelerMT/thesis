@article{Kobayashi2012,
  title = {Abe Homotopy Classification of Topological Excitations under the Topological Influence of Vortices},
  author = {Kobayashi, Shingo and Kobayashi, Michikazu and Kawaguchi, Yuki and Nitta, Muneto and Ueda, Masahito},
  year = {2012},
