@article{Abo-Shaeer2001,
  title = {Observation of {{Vortex Lattices}} in {{Bose-Einstein Condensates}}},
  author = {{Abo-Shaeer}, J. R. and Raman, C. and Vogels, J. M. and Ketterle, W.},
